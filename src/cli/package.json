    "oclif": "^4.1.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/carloshpds/my-worktime",
  "license": "MIT",
  "main": "",
  "name": "my-worktime",
  "oclif": {
    "bin": "my-worktime",
    "dirname": "my-worktime",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": ":"
  },
  "repository": "carloshpds/my-worktime",
  "scripts": {
    "build": "shx rm -rf dist && npm run build:messages && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "jest --config=jest.config.cjs",
    "version": "oclif readme && git add README.md",
    "build:messages": "messageformat src/tools/i18n/messages --locale=en-US,pt-BR --options.customFormatters=./src/tools/i18n/messages/customFormatters.cjs --outfile=src/tools/i18n/messages/compiledMessages.js"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/carloshpds/my-worktime/issues",
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module",
  "keywords": [
    "worktime",
    "my-worktime",
    "mw",
    "time",
    "hour",
    "minute",
    "ahgora",
    "work",
    "utility",
    "oclif"
  ]
